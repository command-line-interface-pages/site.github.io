      "required": [
        "url"
      ],
      "properties": {
        "url": {
          "description": "A project pull request url",
          "type": "string",
          "minLength": 1,
          "examples": [
            "https://github.com/srsudar/eg/pull/97"
          ]
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
